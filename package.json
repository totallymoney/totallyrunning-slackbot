{
  "name": "totallyrunning-slackbot",
  "version": "1.1.0",
  "description": "A slackbot for the TotallyRunning Strava club, for TotallyMoney employees",
  "main": "handler.js",
  "scripts": {
    "lint": "eslint --quiet --ext .js src",
    "lint:fix": "yarn run lint --fix",
    "test": "serverless-bundle test",
    "dev": "serverless offline start --stage dev",
    "deploy": "serverless deploy --stage prod"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomoakley/totallyrunning-slackbot"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "serverless-bundle": "^1.2.5",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.3.3",
    "serverless-offline-ssm": "^5.0.2"
  },
  "dependencies": {
    "aws-sdk": "^2.814.0",
    "date-fns": "^2.14.0",
    "eslint-config-prettier": "^6.11.0",
    "node-fetch": "^2.6.1",
    "qs": "^6.9.4",
    "serverless": "^1.72.0",
    "simple-oauth2": "3.4.0"
  },
  "lint-staged": {
    "./src/**/*.js": [
      "yarn lint",
      "pretty-quick --staged"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
